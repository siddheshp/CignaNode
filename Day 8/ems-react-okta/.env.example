# Okta Configuration
# Copy this file to .env and fill in your actual values

# Your Okta domain (e.g., https://dev-12345678.okta.com or https://integrator-1649836.okta.com)
# Note: Do NOT include "-admin" in the domain - use the base domain without "-admin"
VITE_OKTA_DOMAIN=https://your-okta-domain.okta.com

# Your Okta application client ID
VITE_OKTA_CLIENT_ID=your-client-id-here

# Set to true for local development (disable HTTPS check)
VITE_OKTA_DISABLE_HTTPS_CHECK=false
